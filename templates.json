  {
    "type": "container",
    "title": "Gitlab CE",
    "description": "Open-source end-to-end software development platform",
    "note": "Default username is <b>root</b>. Check the <a href=\"https://docs.gitlab.com/omnibus/docker/README.html#after-starting-a-container\" target=\"_blank\">Gitlab documentation</a> to get started.",
    "categories": ["development", "project-management", "vault", "cicd"],
    "platform": "linux",
    "logo": "https://cloudinovasi.id/assets/img/logos/gitlab_ce.png",
    "image": "gitlab/gitlab-ce:latest",
    "env": [
      { "name": "GITLAB_HOST",
        "label": "full url of instance - ie http/https..$VIRTUAL_HOST"
      },
      { "name": "VIRTUAL_HOST",
        "label": "DNS hostname"
      },
      {
        "name": "VIRTUAL_PORT",
        "set": "80"
      },
      {
        "name": "VIRTUAL_PROTO",
        "set": "http"
      },
      {
        "name": "GITLAB_OMNIBUS_CONFIG",
        "set": "external_url 'http://gitlab.dms.local'\ngitlab_rails['gitlab_shell_ssh_port'] = 2222\ngitlab_rails['ldap_enabled'] = true\ngitlab_rails['ldap_servers'] = YAML.load <<-EOS\nmain: # 'main' is the GitLab 'provider ID' of this LDAP server\nlabel: 'DC1'\nhost: '192.168.0.33'\nport: 389\nuid: 'sAMAccountName'\nencryption: 'start_tls'\nverify_certificates: false\nbind_dn: 'svc_cicd'\npassword: '3bars4life'\nactive_directory: true\ngroup_base: 'OU=Applications,OU=Groups,DC=dms,DC=local'\nblock_auto_created_users: false\nallow_username_or_email_login: false\nlowercase_usernames: true\nbase: 'OU=Members,DC=dms,DC=local'\nsync_ssh_keys: true\nuser_filter: '(memberOf:1.2.840.113556.1.4.1941:=CN=CICD Users,OU=Applications,OU=Groups,DC=dms,DC=local)'\nEOS"
      },
      {
        "name": "GITLAB_ROOT_PASSWORD",
        "label": "Root Password"
      }
    ],
    "restart_policy": "on-failure",
    "ports": [
      "443/tcp",
      "22/tcp"
    ],
    "volumes": [
      { "container": "/etc/gitlab" },
      { "container": "/var/log/gitlab" },
      { "container": "/var/opt/gitlab" }
    ]
  },
